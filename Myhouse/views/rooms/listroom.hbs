<div class="backgroundlist">
  <div class="row">

    <div class="col-md-7 row" style="margin: 0;">
<div style=" margin-top: 10px; margin-left: 10px;" class="row">
  {{#each room}}

      <div style="width: 50%;  margin-top: 15px;  border-radius: 20px; cursor: pointer;  " class="card " >

        <img style="padding: 6px; border-radius: 20px;" src="{{this.imageP}}" class="card-img-top img-md"  />
  
        <div class="card-body">
          <p style="font-size: 15px;">{{this.tipo}} - {{this.hombres}} compa√±ero de piso</p>
          <div>
          <h5 class="card-title" style="margin-top: -1%; line-height: 20px; width: calc(100% - 16px); font-size: 20px; white-space: nowrap; overflow: hidden;
    text-overflow: ellipsis;" >{{this.titulo}}</h5>
         </div>
         <div class="row">
         <div style="border-right: 1px solid;padding: 5px; text-align: center;" class="mt-3 col-md-3">
          <p class="card-text" style="font-size: 13px; font-family: 'Roboto', sans-serif; ">
            RENTA
            <br>
            {{this.renta}}$/mes
          </p>
          </div>
          <div style="border-right: 1px solid;  padding: 5px; text-align: center;" class="mt-3 col-md-3">
          <p class="card-text" style=" font-size: 13px;font-family: 'Roboto', sans-serif;">
            DEPOSITO
            <br>

            {{this.deposito}}$/anual
          
          </p>
          </div>
          <div style="border-right: 1px solid;  padding: 5px; text-align: center;" class="mt-3 col-md-3" >
          <p class="card-text" style=" font-size: 13px;font-family: 'Roboto', sans-serif;">
            BANOS
            <br>
          {{this.banos}}
          </p>
          </div>
          <div style=" padding: 5px; text-align: center;" class="mt-3 col-md-3">
          <p class="card-text" style=" font-size: 13px;font-family: 'Roboto', sans-serif;">
            CUARTOS
            <br>
            {{this.cuartos}}
          </p>
          </div>
          </div>
   
        </div>
<form action="/room/{{this._id}}" method="GET">
        <div style="display: flex; justify-content: center;">
       <button type="submit" class="btn btn-outline-secondary mb-1 mt-2" style="border-radius: 30px; width: 80%;  ">Ver mas</button>
</div>
</form>
      </div>

{{/each}}


    </div>
    </div>

    <div class="col-md-5" style="margin: 0;">
      <iframe
        class="maplistimg"
        src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d481728.80465062865!2d-99.42382104334503!3d19.390519015859844!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x85ce0026db097507%3A0x54061076265ee841!2zQ2l1ZGFkIGRlIE3DqXhpY28sIENETVgsIE3DqXhpY28!5e0!3m2!1ses!2sve!4v1621455527833!5m2!1ses!2sve"
        width="220"
        height="450"
        allowfullscreen=""
        loading="lazy"
      ></iframe>

    </div>
  </div>

</div>

