    <div class ="moviedesc">
        <div> <img src="{{movie.image}}"></div>      
        <div class = "movietext">
            <h2> {{movie.title}}</h1>
            <h3> Director: {{movie.director}}</h3>
            <p>Stars:</p>
            <ul>
              {{#each movie.stars}}
            <li> {{this}}</li>
            {{/each}}
            </ul>
            <p> {{movie.description}}</p>
            <p>Movie Showtimes:</p>
            <ul>
             {{#each movie.showtimes}}
             <li> {{this}}</li>
              {{/each}}
              </ul>

        </div>
        </div>
   

