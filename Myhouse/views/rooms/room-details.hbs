  <div class="d-flex flex-row justify-content-around">

   

 

    
    <div class="d-flex flex-column">
      <h5> Cuartos </h5>
      <div class="d-flex flex-column">
        <img class="iconroom" src="https://img.icons8.com/pastel-glyph/64/000000/single-bed--v6.png">
        <br>
        <p>{{cuartos}} Habitaciones</p>
      </div>
    </div>

   

    <div class="d-flex flex-column">
      <h5> Banos</h5>
      <div class="d-flex flex-column">
        <img class="iconroom" src="https://img.icons8.com/ios/100/000000/sink.png">
        <br>
        <p>{{banos}} Banos</p>
      </div>
    </div>

  </div>
</div>
